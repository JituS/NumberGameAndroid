<?xml version="1.0" encoding="utf-8"?>

<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center"

    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.jitendrasachdeva.newtest.MainActivity"
    android:background="#e7e5e5">

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/empty"
        android:id="@+id/button"
        android:height="80dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:background="@color/switch_thumb_disabled_material_dark"
        android:textSize="20dp"
        android:textStyle="bold"
        android:layout_marginLeft="2dp"
        android:layout_marginTop="2dp"
        android:textColor="#ffffff" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:height="80dp"
        android:id="@+id/button2"
        android:layout_below="@+id/button"
        android:layout_alignParentStart="true"
        android:layout_alignEnd="@+id/button"
        android:background="@color/switch_thumb_disabled_material_dark"
        android:textSize="20dp"
        android:textStyle="bold"
        android:layout_marginLeft="2dp"
        android:layout_marginTop="2dp"
        android:textColor="#ffffff" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:height="80dp"
        android:id="@+id/button3"
        android:layout_below="@+id/button2"
        android:layout_alignParentStart="true"
        android:layout_alignEnd="@+id/button2"
        android:background="@color/switch_thumb_disabled_material_dark"
        android:textSize="20dp"
        android:textStyle="bold"
        android:layout_marginLeft="2dp"
        android:layout_marginTop="2dp"
        android:textColor="#ffffff" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:height="80dp"

        android:id="@+id/button5"
        android:layout_alignParentTop="true"
        android:layout_toEndOf="@+id/button"
        android:background="@color/switch_thumb_disabled_material_dark"
        android:textSize="20dp"
        android:textStyle="bold"
        android:layout_marginLeft="2dp"
        android:layout_marginTop="2dp"
        android:textColor="#ffffff" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button6"
        android:height="80dp"

        android:layout_below="@+id/button"
        android:layout_toEndOf="@+id/button2"
        android:background="@color/switch_thumb_disabled_material_dark"
        android:textSize="20dp"
        android:textStyle="bold"
        android:layout_marginLeft="2dp"
        android:layout_marginTop="2dp"
        android:textColor="#ffffff" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button7"
        android:height="80dp"

        android:layout_below="@+id/button6"
        android:layout_toStartOf="@+id/button10"
        android:background="@color/switch_thumb_disabled_material_dark"
        android:textSize="20dp"
        android:textStyle="bold"
        android:layout_marginLeft="2dp"
        android:layout_marginTop="2dp"
        android:textColor="#ffffff" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button8"
        android:height="80dp"

        android:layout_below="@+id/button6"
        android:layout_toEndOf="@+id/button7"
        android:background="@color/switch_thumb_disabled_material_dark"
        android:textSize="20dp"
        android:textStyle="bold"
        android:layout_marginLeft="2dp"
        android:layout_marginTop="2dp"
        android:textColor="#ffffff" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button9"
        android:height="80dp"

        android:background="@color/switch_thumb_disabled_material_dark"
        android:textSize="20dp"
        android:textStyle="bold"
        android:layout_marginLeft="2dp"
        android:layout_alignTop="@+id/button5"
        android:layout_toEndOf="@+id/button5"
        android:textColor="#ffffff" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button10"
        android:height="80dp"

        android:layout_below="@+id/button5"
        android:layout_toEndOf="@+id/button6"
        android:background="@color/switch_thumb_disabled_material_dark"
        android:textSize="20dp"
        android:textStyle="bold"
        android:layout_marginLeft="2dp"
        android:layout_marginTop="2dp"
        android:textColor="#ffffff" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:height="80dp"
        android:id="@+id/button11"
        android:layout_below="@+id/button3"
        android:layout_toStartOf="@+id/button7"
        android:background="@color/switch_thumb_disabled_material_dark"
        android:textSize="20dp"
        android:textStyle="bold"
        android:layout_marginLeft="2dp"
        android:layout_marginTop="2dp"
        android:textColor="#ffffff" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:height="80dp"
        android:id="@+id/button12"
        android:layout_below="@+id/button8"
        android:layout_toEndOf="@+id/button7"
        android:background="@color/switch_thumb_disabled_material_dark"
        android:textSize="20dp"
        android:textStyle="bold"
        android:layout_marginLeft="2dp"
        android:layout_marginTop="2dp"
        android:textColor="#ffffff" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:height="80dp"
        android:id="@+id/button13"
        android:background="@color/switch_thumb_disabled_material_dark"
        android:textSize="20dp"
        android:textStyle="bold"
        android:layout_marginLeft="2dp"
        android:layout_alignTop="@+id/button11"
        android:layout_toEndOf="@+id/button11"
        android:textColor="#ffffff" />

    <EditText
            android:layout_width="260dp"
            android:layout_height="wrap_content"
            android:id="@+id/editText"
            android:layout_gravity="left|bottom"
            android:text="Touch numbers in sequence"
            android:background="#42e8e3e5"
            android:textColor="#2d0101"
            android:textSize="20dp"
        android:layout_below="@+id/button11"
        android:layout_alignEnd="@+id/button12"
        android:textStyle="bold"
        android:layout_marginTop="3dp"
        android:layout_marginLeft="2dp"
        android:autoText="false" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Restart"
        android:id="@+id/restart"
        android:layout_below="@+id/editText"
        android:layout_toEndOf="@+id/button11"
        android:textStyle="bold"
        android:enabled="true" />


</RelativeLayout>
